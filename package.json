{
  "name": "guia-hoteles",
  "version": "1.0.0",
  "description": "Nuestro primer proyecto NODEJS",
  "main": "index.html",
  "scripts": {
    "dev": "lite-server",
    "start": "concurrently \"npm run watch:scss\"  \"npm run dev\" ",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass -o css/ css/ ",
    "watch:scss": "onchange 'css/style.scss' -- echo 'hola' ",
    "clean": "rimraf dist",
    "imagemin": "imagemin images/* --out-dir dist/images",
    "usemin": "usemin index.html -d dist --htmlmin -o dist/index.html && usemin about.html -d dist --htmlmin -o dist/about.html && usemin precio.html -d dist --htmlmin -o dist/precio.html && usemin contacto.html -d dist --htmlmin -o dist/contacto.html && usemin terminos.html -d dist --htmlmin -o dist/terminos.html ",
    "build": "npm run clean & npm run scss & npm run imagemin & npm run usemin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oibecerra/guia-hoteles.git"
  },
  "author": "Oliver Becerra A.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/oibecerra/guia-hoteles/issues"
  },
  "homepage": "https://github.com/oibecerra/guia-hoteles#readme",
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "cssmin": "^0.4.3",
    "grunt": "^1.0.3",
    "grunt-browser-sync": "^2.2.0",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-concat": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^2.2.1",
    "grunt-contrib-imagemin": "^4.0.0",
    "grunt-contrib-sass": "^1.0.0",
    "grunt-contrib-uglify": "^4.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-filerev": "^2.3.1",
    "grunt-usemin": "^3.1.1",
    "htmlmin": "^0.0.7",
    "jit-grunt": "^0.10.0",
    "lite-server": "^2.6.1",
    "node-sass": "^9.0.0",
    "onchange": "^7.1.0",
    "rimraf": "^2.6.2",
    "time-grunt": "^1.4.0",
    "uglifyjs": "^2.4.11",
    "usemin-cli": "^0.6.0"
  },
  "dependencies": {
    "bootstrap": "^4.6.2",
    "concurrently": "^8.2.2",
    "jquery": "^3.7.1",
    "open-iconic": "^1.1.1",
    "popper.js": "^1.14.3"
  }
}
